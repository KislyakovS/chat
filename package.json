    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "pug": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "targets": {
    "frontend": {
      "context": "browser",
      "source": [
        "src/pages/login.pug",
