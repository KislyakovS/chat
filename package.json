  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KislyakovS/chat/issues"
  },
  "homepage": "https://github.com/KislyakovS/chat#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@parcel/transformer-pug": "^2.7.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.16",
    "@types/pug": "^2.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.2",
